--creating a method
--Ctrl+X Ctrl+F creates a file
--Ctrl+X Ctrl+S saves a file
needsPackage"RandomIdeal"
needsPackage"Depth"

testCM = method()
testCM(ZZ):=(n) -> (
    local kk;
    local S;
    local L;
    local m;
    local J;
    local i;
    local cmideal;
    local R;
    
    cmideal=0;
    kk=ZZ/101;
    S=kk[a..e];
    L={3,3,4,6};
    m=matrix{{a^3,b^4+c^4,d^5}};
    
    --for i from 1 to n do 
    --(
--	J=randomIdeal(L,m);
--	R=ring J;
--	J=(R^1)/J;
--	return isCM(J);
--	)
    J=apply(n, J->randomIdeal(L,m));
    for i from 1 to n do
    (
	if isCM(J#(i-1)) then
	    cmideal=cmideal+1;
	)
    
    return cmideal;    
    )
end

restart
load "demo.m2"
--installPackage"RandomIdeal"
testCM(10)


